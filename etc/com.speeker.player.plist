    <key>StandardOutPath</key>
    <string>/tmp/speeker-player.out</string>
    <key>StandardErrorPath</key>
    <string>/tmp/speeker-player.err</string>
    <key>ProcessType</key>
    <string>Interactive</string>
</dict>
</plist>
